        <div class="howSection">
                <div class="col-lg-12 text-center">
                    <h2 class="section-headin sectionHeading">How Anautomy Works</h2>
                    <h3 class="section-subheading text-muted"></h3>
                </div>
                <div class="col-md-4 text-center">
                  
                    <span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-search fa-stack-1x fa-inverse"></i>
                    </span>
                  
                    <h4 class = "howLabels">Find</h4>
                    <p class="text-muted howDescriptions">Find your car and the repair you need through our powerful search tool.</p>
                </div>
                <div class="col-md-4 text-center">
                  
                    <span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-eye fa-stack-1x fa-inverse "></i>
                    </span> 
                  
                    <h4 class="howLabels">Browse</h4>
                    <p class="text-muted howDescriptions">See how much others are paying on the same repair for your vehicle.</p>
                </div>
                <div class="col-md-4 text-center">
                  
                    <span class="fa-stack fa-4x">
                        <i class="fa fa-circle fa-stack-2x text-primary"></i>
                        <i class="fa fa-usd fa-stack-1x fa-inverse"></i>
                    </span>
                
                    <h4 class="howLabels">Save</h4>
                    <p class="text-muted howDescriptions">Find the best place to get the repair done for a price that you know is fair.</p>
                </div>
            <h5 class = "text-center">We'll be launching soon. Get on our email list for an invitation to our beta launch.</h5>
            
         <%= form_for @subscriber, :html => {:class => 'cold-md-3 col-md-offset-3 form-inline'}, remote: true do |f|%>
         <%= render 'shared/error_messages' %>
  <div class="form-group">
    <label class="sr-only">Email</label>
   <%= f.email_field :email, class: 'form-control input-lg', :placeholder => "Email"%>
   <label class="sr-only">Zip Code</label>
   <%= f.text_field :zip, class: 'form-control input-lg', :placeholder => "Zip code"%>
  </div>
  <div class="form-group">
    <%= f.submit "Notify Me", class: "btnbtn btn-lg btn-primary btn-block" %>

</div>
  </div>
  <% end %>

        </div>
    </div>
  </div>
   
