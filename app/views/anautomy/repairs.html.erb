    <script>
$("select").selectOrDie();

$("#repairCategories").accordion({
      collapsible: true,
      active: false,
      animate: 450,
      heightStyle: "content",
      header: "h3",
      fillSpace: true
  });

$("#engineRepairs, #exhaustEmissionsRepairs, #brakesRepairs, #electricalLightsRepairs, #suspensionSteeringRepairs, #suspensionSteeringRepairs, #transmissionRepairs").accordion({
      collapsible: true,
      active: false,
      animate: 450,
      heightStyle: "content",
      header: "h4",
      fillSpace: true
    });

$( "#top_repair").accordion({
      collapsible: true,
      active: true,
      animate: 450,
      heightStyle: "content",
      header: "h4",
      fillSpace: true
    });

$( "#top_repair2").accordion({
      collapsible: true,
      active: true,
      animate: 450,
      heightStyle: "content",
      header: "h4",
      fillSpace: true
    });
$( "#top_repair3").accordion({
      collapsible: true,
      active: true,
      animate: 450,
      heightStyle: "content",
      header: "h4",
      fillSpace: true
    });

jQuery('#repairCategories h3').click(function() {
    jQuery(this).toggleClass('active').find('i').toggleClass('fa-angle-right fa-angle-down')
           .closest('h3').siblings('h3')
           .removeClass('active').find('i')
           .removeClass('fa-angle-right').addClass('fa-angle-down');


     jQuery(this).next('.repairName').slideToggle().siblings('.repairName').slideUp();
});

$("#range").ionRangeSlider({
            hide_min_max: true,
            keyboard: true,
            min: 0,
            max: 1500,
            from: <%= @low_range %>,
            to: <%= @high_range %>,
            disable: true,
            type: 'double',
            step: 1,
            prefix: "$",
            grid: false,
             prettify_enabled: true,
             prettify_separator: ","
        });



        $("#range2, #range3, #range4").ionRangeSlider({
