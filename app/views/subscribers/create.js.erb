<% if @subscriber.save %>
swal({   
	title: "Success",   
	text: "You're on our list.",   
	type: "success",   
	confirmButtonText: "Cool" });
$('#subscriber_form').trigger("reset");
<% else %>
swal({   
	title: "Error",   
	text: "Please include both email and zip code.",   
	type: "error",   
	confirmButtonText: "Okay" });
<%end%>